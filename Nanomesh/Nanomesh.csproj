<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Configurations>Debug;Release;Obfuscated;Native;Symbols</Configurations>
	<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	<PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Symbols|AnyCPU'">
    <Optimize>false</Optimize>
    <DefineConstants>TRACE</DefineConstants>
  </PropertyGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="if $(ConfigurationName) == Obfuscated (&#xD;&#xA;    &quot;..\Tools\Confuser\Confuser.CLI.exe&quot; Obfuscation.crproj&#xD;&#xA;)" />
  </Target>

    <Target Name="PostBuild" AfterTargets="PostBuildEvent">
	    <Exec Command="copy /Y &quot;$(TargetDir)Nanomesh.dll&quot; &quot;$(SolutionDir)Package\Nanomesh.dll&quot;&#xD;&#xA;copy /Y &quot;$(TargetDir)Nanomesh.pdb&quot; &quot;$(SolutionDir)Package\Nanomesh.pdb&quot;" />
    </Target>

</Project>
